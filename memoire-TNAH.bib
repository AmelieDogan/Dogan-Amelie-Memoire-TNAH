@phdthesis{hamadTrappedUpgradeCycle2025,
  title = {Trapped in the {{Upgrade Cycle}}: {{An Investigation}} of {{Digital Planned Obsolescence}} and Its {{Consequences}}},
  shorttitle = {Trapped in the {{Upgrade Cycle}}},
  author = {Hamad, Roja},
  year = {2025},
  journal = {Trapped in the Upgrade Cycle: An Investigation of Digital Planned Obsolescence and its Consequences},
  urldate = {2025-08-17},
  abstract = {This study examines digital planned obsolescence (DPO) through the lens of the recent Henrik Huseby v Apple lawsuit. The case highlights how corporate decisions from software updates that deliberately slow older devices to hardware designs that limit repair can intentionally shorten the lifespan of electronics. Combining analysis of legal documents, interviews with experts, and a review of policy trends, the research uncovers the broad impacts of DPO. It finds that industry-wide practices contribute to the production of over fifty million tons of electronic waste (e-waste) globally each year, accelerate the depletion of critical natural resources, and reinforce inequalities as consumers are locked into costly upgrade cycles. The Huseby case exemplifies how companies use both legal restrictions and technical tricks to retain control over product lifecycles and keep independent repairers at bay. The findings reveal significant gaps in current regulations. Aggressive corporate lobbying has so far weakened right-to-repair (R2R) legislation even as concerns about sustainability mount. To address this, the thesis proposes concrete reforms: for example, enforceable design standards that make devices easier to fix and stronger extended producer responsibility rules that hold manufacturers accountable for end-of-life impacts. By bringing together legal, environmental, and technological perspectives, this work positions DPO not merely as a business strategy but as a pressing global challenge for sustainability, calling for coordinated systemic change.},
  langid = {english},
  school = {The University of Bergen},
  keywords = {Technologies}
}

