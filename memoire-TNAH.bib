@book{smithReliabilityMaintainabilityRisk2011,
  title = {Reliability, {{Maintainability}} and {{Risk}}: {{Practical Methods}} for {{Engineers Including Reliability Centred Maintenance}} and {{Safety-Related Systems}}},
  shorttitle = {Reliability, {{Maintainability}} and {{Risk}}},
  author = {Smith, David J.},
  year = {2011},
  publisher = {Elsevier Science \& Technology},
  address = {Chantilly, UNITED KINGDOM},
